<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.dachuang.dao.VertexDao">
    <select id="findAll" resultType="com.example.dachuang.entity.Vertex">
        SELECT * FROM tb_vertex
    </select>
    <select id="SearchVertexByName" resultType="com.example.dachuang.entity.Vertex">
        SELECT * FROM tb_vertex WHERE name = #{name}
    </select>
</mapper>